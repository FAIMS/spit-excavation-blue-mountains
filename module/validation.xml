      <validator type="blankchecker">
        <param value="measure" type="field"/>
      </validator>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Unit">
    <property name="Unit ID">
      <validator type="blankchecker">
        <param value="measure" type="field"/>
      </validator>
