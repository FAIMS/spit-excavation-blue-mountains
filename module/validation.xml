      <validator type="blankchecker">
        <param value="measure" type="field"/>
      </validator>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Note">
    <property name="Note Label">
      <validator type="blankchecker">
        <param value="measure" type="field"/>
      </validator>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Artefact">
    <property name="Artefact Spit ID">
      <validator type="blankchecker">
        <param value="measure" type="field"/>
