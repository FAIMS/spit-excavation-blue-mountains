        <input ref="Artefact_Type" faims_attribute_name="Artefact Type" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>Artefact Type</label>
        </input>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="Photo" faims_attribute_name="Photo">
          <label>{Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Photo" faims_style_class="faims-blue">
          <label>{Button_Photo}</label>
        </trigger>
        <input ref="Notes" faims_attribute_name="Notes" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Notes}</label>
        </input>
        <input ref="Pantone_ID" faims_attribute_name="Pantone ID" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
