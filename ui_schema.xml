        </input>
        <input faims_attribute_type="measure" ref="Artefact_ID" faims_attribute_name="Artefact ID" faims_style_class="required">
          <label>Artefact ID</label>
        </input>
        <input faims_attribute_type="measure" ref="X" faims_attribute_name="X">
